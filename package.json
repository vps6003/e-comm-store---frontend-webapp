{
  "name": "webapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "server-start": "node vpsEkart.js",
    "dev": "ng serve --port 4200",
    "dev-build": "ng build --configuration development --base-href",
    "build": "ng build --configuration production --base-href",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "gh-deploy": "ng build --configuration production --base-href https://vps6003.github.io/e-comm-store---frontend-webapp/ && npx angular-cli-ghpages --dir=dist/webapp/browser",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:changed": "node ./tools/lint-changed.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/material": "^20.2.7",
    "@angular/platform-browser": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngrx/store-devtools": "^20.1.0",
    "@tailwindcss/postcss": "^4.1.12",
    "express": "^5.1.0",
    "ngx-owl-carousel-o": "^20.0.1",
    "ngx-pagination": "^6.0.3",
    "ngx-toastr": "^19.1.0",
    "nodemon": "^3.1.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular-devkit/build-angular": "^20.3.4",
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.4",
    "@briebug/jest-schematic": "^6.0.0",
    "@ngrx/schematics": "^20.1.0",
    "@types/jest": "29.5.14",
    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "jest": "29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "jest-preset-angular": "^14.1.0"
  }
